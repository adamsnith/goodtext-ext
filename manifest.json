{
  "manifest_version": 2,
  "name": "GoodText",
  "version": "1",
  "icons": { "16": "asdf.png", "48": "asdf.png", "128": "asdf.png" },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["/js/jquery-3.3.1.min.js", "content.js"]
    }
  ],
  "web_accessible_resources": ["open.otf"],
  "browser_action": {
    "default_icon": "asdf.png",
    "default_popup": "popup.html",
    "default_title": "GoodText Options"
  }
}
